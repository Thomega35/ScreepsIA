{
  "name": "screeps-ia",
  "version": "1.0.0",
  "type": "module",
  "description": "Thomega35's script for Screeps",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "rollup -c",
    "push-main": "rollup -c --environment DEST:main",
    "push-pserver": "rollup -c --environment DEST:pserver",
    "push-season": "rollup -c --environment DEST:season",
    "push-sim": "rollup -c --environment DEST:sim",
    "test-integration": "echo 'See docs/in-depth/testing.md for instructions on enabling integration tests'",
    "watch-main": "rollup -cw --environment DEST:main",
    "watch-pserver": "rollup -cw --environment DEST:pserver",
    "watch-season": "rollup -cw --environment DEST:season",
    "watch-sim": "rollup -cw --environment DEST:sim"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thomega35/ScreepsIA.git"
  },
  "author": "",
  "license": "Unlicense",
  "overrides": {
    "glob": "10.4.2",
    "rimraf": "6.0.1"
  },
  "bugs": {
    "url": "https://github.com/Thomega35/ScreepsIA/issues"
  },
  "homepage": "https://github.com/Thomega35/ScreepsIA#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@types/lodash": "4.17.9",
    "@types/node": "^22.7.4",
    "@types/screeps": "^3.3.8",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@typescript-eslint/typescript-estree": "^8.8.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "rollup": "^4.23.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-screeps-world": "^1.0.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "source-map": "~0.7.4"
  }
}
